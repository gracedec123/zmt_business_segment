<mvc:View controllerName="webapp.webapp.controller.View1" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:table="sap.ui.table" xmlns:f="sap.ui.layout.form" xmlns:u="sap.ui.unified"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App id="app">
		<pages>
			<Page id="page" title="{i18n>titleMain}">
				<content>
					<FlexBox height="100px" alignItems="End" justifyContent="Center">
						<items></items>
					</FlexBox>
					<FlexBox height="100px" alignItems="End" justifyContent="Center">
						<items>
							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="MT Business Sectors" press="onMT">
								<tileContent>
									<TileContent unit="" footer="">
										<ImageContent src="sap-icon://business-objects-mobile"/>
									</TileContent>
								</tileContent>
							</GenericTile>
							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Initial Data" press="onInitial">
								<tileContent>
									<TileContent unit="" footer="">
										<ImageContent src="sap-icon://initiative"/>
									</TileContent>
								</tileContent>
							</GenericTile>
							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Final Data" press="onFinal">
								<tileContent>
									<TileContent unit="" footer="">
										<ImageContent src="sap-icon://detail-view"/>
									</TileContent>
								</tileContent>
							</GenericTile>
						</items>
					</FlexBox>
				</content>
				<footer>
					<Bar id="__bar0">
						<customData>
							<core:CustomData key="sap-ui-fastnavgroup" value="true" writeToDom="true" id="__dataso"/>
						</customData>
					</Bar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>