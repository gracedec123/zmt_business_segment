<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialogCusDesc"
		title="{i18n>SoldToDesc}"
		items="{oCusDesc>/}"
		class="F4height_multi"
		search=".onValueHelpSearchCusDesc"
		confirm=".onValueHelpCloseCusDesc"
		cancel=".onValueHelpCloseCusDesc"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{oCusDesc>SOLD_TO_DESC}" 
			description="{oCusDesc>SOLD_TO}"/>
	</SelectDialog>
</core:FragmentDefinition>